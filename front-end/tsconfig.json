{
  "compilerOptions": {
    "outDir": "./dist/",
    "noImplicitAny": true,
    "resolveJsonModule": true,
    "module": "es6",
    "target": "es5",
    "jsx": "react",
    "allowJs": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "moduleResolution": "node",
  },
  "exclude": ["node_modules", "webpack.config.js"]
}